<% if user_signed_in? %>
  <div class="navbar navbar-expand-sm navbar-light navbar-lewagon">
    <%= link_to "#", class: "navbar-brand" do %>
      <%= image_tag "data:image/png;base64," %>
      <% end %>

    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>


    <div class="collapse navbar-collapse" id="navbarSupportedContent">
      <ul class="navbar-nav mr-auto">
        <li class="nav-item active">
          <%= link_to "Início", bus_travels_path, class: "nav-link" %>
        </li>
        <li class="nav-item dropdown">
          <%= image_tag "https://kitt.lewagon.com/placeholder/users/Maykhodr", class: "avatar dropdown-toggle", id: "navbarDropdown", data: { toggle: "dropdown" }, 'aria-haspopup': true, 'aria-expanded': false %>
          <div class="dropdown-menu dropdown-menu-right" aria-labelledby="navbarDropdown">
            <% if current_user.admin? %>
              <%= link_to "Criar usuário", new_user_registration_path, class: "dropdown-item" %>
              <%= link_to "Empresas", companies_path, class: "dropdown-item" %>
              <%= link_to "Criar empresa", new_company_path, class: "dropdown-item" %>
              <%= link_to "Criar passageiro", new_passenger_trip_path, class: "dropdown-item" %>
              <%#= link_to "Criar linha", new_company_travel_line_path, class: "dropdown-item" %>
              <%#= link_to "Viagens", company_travel_line_bus_travels_path(@travel_line), class: "dropdown-item" %>
            <% end %>
            <%= link_to "Log out", destroy_user_session_path, method: :delete, class: "dropdown-item" %>
          </div>
        </li>
      </ul>
    </div>
  </div>
<% end %>
